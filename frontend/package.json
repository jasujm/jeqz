{
    "name": "jeqz-frontend",
    "version": "0.1.0",
    "description": "Jaakko's equation quiz frontend",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@testing-library/user-event": "^12.8.3",
        "@types/jest": "^26.0.23",
        "@types/node": "^12.20.11",
        "@types/react": "^17.0.4",
        "@types/react-dom": "^17.0.3",
        "axios": "^0.21.1",
        "bootstrap": "^4.6.0",
        "classnames": "^2.3.1",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-bootstrap": "^1.5.2",
        "react-dom": "^17.0.2",
        "react-mathjax": "^1.0.1",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.3",
        "remove": "^0.1.5",
        "sass": "^1.32.12",
        "typescript": "^4.2.4",
        "web-vitals": "^1.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint src/",
        "format": "prettier -w src"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "rules": {
            "jest/valid-expect": "off"
        },
        "overrides": [
            {
                "files": [
                    "*.spec.tsx"
                ],
                "rules": {
                    "@typescript-eslint/no-unused-expressions": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/chai": "^4.2.18",
        "@types/chai-dom": "0.0.10",
        "@types/lodash": "^4.14.169",
        "@types/react-mathjax": "^1.0.0",
        "@types/react-router-dom": "^5.1.7",
        "@types/sinon-chai": "^3.2.5",
        "axios-mock-adapter": "^1.19.0",
        "chai": "^4.3.4",
        "chai-dom": "^1.9.0",
        "prettier": "^2.3.0",
        "sinon": "^10.0.0",
        "sinon-chai": "^3.6.0"
    }
}
